{
  "parser": "babel-eslint",
  "extends": [],
  "plugins": [],
  "globals": {
    "app": true,
    "logger": true,
    "_": true,
    "sinon": true,
    "expect": true,
    "proxyquire": true,
    "NODE_ENV": true,
    "module": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "rules": {
    "eqeqeq": "error",
    "no-extra-bind": "error",
    "no-extra-semi": "error",
    "no-return-assign": "error",
    "no-trailing-spaces": "error",
    "no-undef": "error",
    "no-alert": "error",
    "no-multi-spaces": "error",
    "semi": [
      "error",
      "never"
    ],
    "no-unreachable": "warn",
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "space-infix-ops": "error",
    "keyword-spacing": "error",
    "valid-typeof": "error",
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "space-before-blocks": [
      "error",
      "always"
    ],
    "brace-style": [
      "error",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "indent": [
      "error", 2,
      {
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        },
        "FunctionExpression": {
          "body": 1,
          "parameters": 2
        },
        "SwitchCase": 1
      }
    ]
  }
}
